{
  "name": "@ahamove/ahaless",
  "description": "Write serverless with Typescript",
  "version": "0.1.1",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jestconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lynkxyz/ahaless.git"
  },
  "bugs": {
    "url": "https://github.com/lynkxyz/ahaless/issues"
  },
  "homepage": "https://github.com/lynkxyz/ahaless#readme",
  "author": "lynkxyz",
  "keywords": [
    "serverless",
    "serverless plugin",
    "serverless typescript"
  ],
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "@abraham/reflection": "^0.7.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0"
  },
  "devDependencies": {
    "@types/jest": "^24.9.0",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.3.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.5"
  }
}
